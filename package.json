{
  "name": "@proxima-one/mangrove-streaming-app",
  "version": "0.1.0",
  "description": "mangrove-streaming-app",
  "license": "MIT",
  "repository": "",
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "keywords": [
    ""
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "clean": "rimraf dist && rimraf coverage",
    "format": "prettier --write \"{src,__tests__}/**/*.ts\"",
    "lint": "eslint ./src --ext .ts",
    "prepublishOnly": "yarn build",
    "prebuild": "yarn clean && yarn format && yarn lint && echo Using TypeScript && tsc --version",
    "build": "tsc --project tsconfig.build.json --pretty",
    "test": "jest --passWithNoTests",
    "coverage": "jest --coverage",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "ts-node -r tsconfig-paths/register src/dev.ts",
    "start:app": "yarn start app start --",
    "watch": "yarn build -- --watch",
    "watch:test": "jest --watch"
  },
  "dependencies": {
    "@proxima-one/proxima-app-runtime": "^0.2.6",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^14.18.4",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.6.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "jest": {
    "preset": "<rootDir>/"
  }
}
