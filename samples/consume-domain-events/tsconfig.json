{
  "compilerOptions": {
    "rootDir": "./",
    "outDir": "./dist",
    "lib": [
      "ES2019"
    ],
    "incremental": false,
    "declaration": true,
    "declarationMap": true,
    "resolveJsonModule": true,
    "target": "ES2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "baseUrl": "./"
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "types": ["node"]
}
